.NextLeftToRight {
    transition-duration: 1s, 1s, 0s;
    transition-delay: 0.1s, 0.1s, 0s;
    transition-timing-function: ease-in-out-elastic, ease-in-out-elastic, ease;
    display: flex;
    visibility: visible;
    overflow: visible;
    transition-property: opacity, scale, translate;
    translate: 250px 0;
    scale: 0 0;
